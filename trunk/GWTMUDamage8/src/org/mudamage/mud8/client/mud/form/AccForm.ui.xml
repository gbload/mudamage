<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="form.css">
		
	</ui:style>
	<g:HTMLPanel>
	    <div>
	        <label>
	            <a class="{style.label}" ui:field="label"></a>
	            <g:ListBox ui:field="kind" name="kind"></g:ListBox>
	        </label>
	        <g:ListBox ui:field="item" name="item"></g:ListBox>
	        <g:ListBox ui:field="sub1"></g:ListBox>
	        <g:ListBox ui:field="sub2"></g:ListBox>
	        <g:ListBox ui:field="sub3"></g:ListBox>
	        <g:ListBox ui:field="sub4"></g:ListBox>
	        <g:ListBox ui:field="sub5"></g:ListBox>
	        <g:ListBox ui:field="sub6"></g:ListBox>
	    </div>
	</g:HTMLPanel>
</ui:UiBinder> 