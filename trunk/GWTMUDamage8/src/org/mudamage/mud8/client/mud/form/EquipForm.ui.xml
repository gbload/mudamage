<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="form.css">
		
	</ui:style>
	<g:HTMLPanel>
	    <div>
	        <label>
	            <a class="{style.label}" ui:field="label"></a>
	            <g:ListBox ui:field="kind" name="kind"></g:ListBox>
	        </label>
	        <g:ListBox ui:field="item" name="item"></g:ListBox>
	        <g:CheckBox ui:field="luck" text="幸運" name="luck"></g:CheckBox>
	        <g:ListBox ui:field="plus" name="plus"></g:ListBox>
	        <g:ListBox ui:field="op" name="op"></g:ListBox>
	        <g:ListBox ui:field="enchant" name="op"></g:ListBox>
	        <g:ListBox ui:field="enchant_option" name="enop"></g:ListBox>
			
			<g:HTMLPanel ui:field="exop_box">
					<a class="{style.label}"></a>
			        <g:ListBox ui:field="exop1"></g:ListBox>
			        <g:ListBox ui:field="exop2"></g:ListBox>
			        <g:ListBox ui:field="exop3"></g:ListBox>
			        <g:ListBox ui:field="exop4"></g:ListBox>
			        <g:ListBox ui:field="exop5"></g:ListBox>
			        <g:ListBox ui:field="exop6"></g:ListBox>
			</g:HTMLPanel>
	    </div>
	</g:HTMLPanel>
</ui:UiBinder> 