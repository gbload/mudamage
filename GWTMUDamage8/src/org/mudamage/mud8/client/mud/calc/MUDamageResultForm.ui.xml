		<div class="{style.rightbox}">
			<a ui:field="pet">デーモン</a>
			<a ui:field="wing"></a>
			<a ui:field="neck"></a>
			<a ui:field="right"></a>
			<a ui:field="left"></a>
			<a ui:field="helm"></a>
			<a ui:field="armor"></a>
			<a ui:field="glove"></a>
			<a ui:field="garter"></a>
			<a ui:field="boots"></a>
			<a ui:field="ring1"></a>
